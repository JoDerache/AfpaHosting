{% include 'base.html.twig' %}
{% include 'shared/header.html.twig' %}
{% include 'shared/sidebar.html.twig' %}
{% block title %}Mon espaces{% endblock %}

{% block body %}
<main id="main" class="main">
{{ form_start(form) }}

<div class="row mb-3">
    <label for="Email" class="col-md-4 col-lg-3 col-form-label">{{form_label(form.mdp)}}</label>
    <div class="col-md-8 col-lg-9">
    {{form_widget(form.mdp)}}
    </div>
</div>

<div class="row mb-3">
    <label for="Email" class="col-md-4 col-lg-3 col-form-label">{{form_label(form.new_password.first)}}</label>
    <div class="col-md-8 col-lg-9">
    {{form_widget(form.new_password.first)}}
    </div>
</div>

<div class="row mb-3">
    <label for="Email" class="col-md-4 col-lg-3 col-form-label">{{form_label(form.new_password.second)}}</label>
    <div class="col-md-8 col-lg-9">
    {{form_widget(form.new_password.second)}}
    </div>
</div>

{{ form_end(form) }}


</main>
{% endblock %}